/* Before using #include to read this file, define a macro:

    PIC32C_MCU (NAME, ARCH, DEV_ATTRIBUTE, MACRO, FPU)

   The NAME is the name of the device, represented as a string constant.

   Some tools assume no whitespace up to the first "," in each entry.
*/

/*
PIC32C_MCU (NAME, ARCH, DEV_ATTRIBUTE, MACRO, FPU)
*/

PIC32C_MCU("ATSAME51G19A",  "cortex-m4",  CORTEX_M4,  "__SAME51G19A__",  "fpv4-sp-d16")
PIC32C_MCU("ATSAME51G18A",  "cortex-m4",  CORTEX_M4,  "__SAME51G18A__",  "fpv4-sp-d16")
PIC32C_MCU("ATSAME51N20A",  "cortex-m4",  CORTEX_M4,  "__SAME51N20A__",  "fpv4-sp-d16")
PIC32C_MCU("ATSAME51N19A",  "cortex-m4",  CORTEX_M4,  "__SAME51N19A__",  "fpv4-sp-d16")
PIC32C_MCU("ATSAME51J19A",  "cortex-m4",  CORTEX_M4,  "__SAME51J19A__",  "fpv4-sp-d16")
PIC32C_MCU("ATSAME51J18A",  "cortex-m4",  CORTEX_M4,  "__SAME51J18A__",  "fpv4-sp-d16")
PIC32C_MCU("ATSAME51J20A",  "cortex-m4",  CORTEX_M4,  "__SAME51J20A__",  "fpv4-sp-d16")

