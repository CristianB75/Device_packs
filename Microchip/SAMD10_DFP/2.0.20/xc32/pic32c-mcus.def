PIC32C_MCU("ATSAMD10D13AS",  "cortex-m0plus",  CORTEX_M0PLUS,  "__SAMD10D13AS__",  NULL)
PIC32C_MCU("ATSAMD10D13AM",  "cortex-m0plus",  CORTEX_M0PLUS,  "__SAMD10D13AM__",  NULL)
PIC32C_MCU("ATSAMD10D14AU",  "cortex-m0plus",  CORTEX_M0PLUS,  "__SAMD10D14AU__",  NULL)
PIC32C_MCU("ATSAMD10C14A",  "cortex-m0plus",  CORTEX_M0PLUS,  "__SAMD10C14A__",  NULL)
